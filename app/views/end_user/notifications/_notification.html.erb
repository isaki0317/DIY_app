<% visitor = notification.visitor %>
<% post = notification.post %>
<div class="user-notice">
  <%= link_to end_user_path(visitor) do %>
    <% if visitor.images.present? %>
      <img src='<%= visitor.images.thumb.url %>' class="post_user_image" alt="プロフ写真">
    <% else %>
      <%= image_tag "sample.jpg", size: '50x50', class: "post_user_image" %>
    <% end %>
  <% end %>
  <%== notification_form(notification) %><span><%= "(#{time_ago_in_words(notification.created_at)}前)" %></span><br>
</div>
