<div class="col-xs-10 col-xs-offset-1"></div>
<div class="col-xs-7 col-xs-offset-1">
  <% if @data.empty? %>
    <p>該当する検索結果はありません。</p>
  <% else %>
    <% if @model == 'end_user' %>
      <%= render 'end_user/end_users/search_index', end_users: @data %>
    <% else %>
      <%= render 'end_user/posts/search_index', public_posts: @data, comment_new: @comment_new %>
    <% end %>
  <% end %>
</div>
<%= render 'post_sidebar', genres: @genres %>

<div class="scroll-event">
  <a href="#" class="scroll-btn-post"><i class="fas fa-long-arrow-alt-up"></i></a>
</div>